        <Key android:keyLabel="b" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#039;"/>
        <Key android:keyLabel="n" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#034;&#241;"/>
        <Key android:keyLabel="m" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="\&#063;&#047;"/>
        <Key android:codes="@integer/keycode_delete" android:iconPreview="@drawable/sym_kb_popup_delete"
                android:keyWidth="15%p" android:keyEdgeFlags="right"
                android:isRepeatable="true"/>
    <Gap> </Gap>
	</Row>
	
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="@integer/keycode_mode"  android:keyLabel="123" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="{&#x2022;&#x0020;&#x2022;&#x0020;&#x2022;}" android:keyWidth="15%p" android:keyEdgeFlags="left"/>
        <Key android:codes="@integer/keycode_any_key" android:iconPreview="@drawable/sym_kb_popup_arrows" android:keyWidth="15%p"/>
        <Key android:keyLabel="," android:keyWidth="11%p" android:popupCharacters="&#047;"/>
        <Key android:codes="32" android:iconPreview="@drawable/sym_kb_popup_space" android:isRepeatable="true" android:keyWidth="33%p"/>
        <Key android:keyLabel="." android:keyWidth="11%p" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="{?!@}"/>
        <Key android:codes="@integer/keycode_action" android:iconPreview="@drawable/sym_kb_popup_return" android:keyWidth="15%p" android:keyEdgeFlags="right"/>
    </Row>
</Keyboard>
    