    <Row>
        <Key android:keyLabel="a" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#033;&#225;&#224;&#226;&#227;&#228;&#229;&#230;" android:horizontalGap="5%p" android:keyEdgeFlags="left" />
