        <Key android:keyLabel="h" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#038;"/>
        <Key android:keyLabel="j" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#042;"/>
        <Key android:keyLabel="k" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#040;"/>
        <Key android:keyLabel="l" android:popupKeyboard="@xml/popup_keyboard" android:popupCharacters="&#041;" android:keyEdgeFlags="right"/>
